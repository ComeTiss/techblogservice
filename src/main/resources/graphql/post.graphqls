schema {
    query: Query
    mutation: Mutation
}

input PostInput {
    id: String
    title: String!
    description: String!
}

type Post {
    id: ID!
    title: String!
    description: String!
}

type Query {
    getAllPosts: [Post]!
}

type Mutation {
    mutatePost(post: PostInput!) : Post
    deletePostsByIds(ids: [ID]!) : [Post]
}



